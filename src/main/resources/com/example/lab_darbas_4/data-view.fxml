      <Button fx:id="exportXlsxButton" layoutX="169.0" layoutY="63.0" mnemonicParsing="false" onAction="#excelExport" text="Export to XLSX file" />
      <Label fx:id="csvLabel" layoutX="18.0" layoutY="96.0" prefHeight="17.0" prefWidth="115.0" />
      <Label fx:id="excelLabel" layoutX="169.0" layoutY="96.0" prefHeight="17.0" prefWidth="113.0" />
   </children>
</AnchorPane>
